digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	menu [label="menu\l" color=red fillcolor=darksalmon peripheries=2]
	help [label="help\l"]
	update [label="update\l"]
	set_web [label="set_web\l"]
	web_acg [label="web_acg\l"]
	web_anime1 [label="web_anime1\l"]
	set_date [label="set_date\l"]
	set_name [label="set_name\l"]
	set_keyword [label="set_keyword\l"]
	menu -> help [label="advance [is_going_to_help]"]
	menu -> update [label="advance [is_going_to_update]"]
	menu -> set_web [label="advance [is_going_to_set_web]"]
	help -> help [label="advance [is_going_to_help]"]
	update -> update [label="advance [is_going_to_show]"]
	update -> set_name [label="advance [is_going_to_set_name]"]
	set_web -> web_acg [label="advance [is_going_to_web_acg]"]
	set_web -> web_anime1 [label="advance [is_going_to_web_anime1]"]
	web_acg -> set_date [label="advance [is_going_to_set_date]"]
	web_anime1 -> set_name [label="advance [is_going_to_set_name]"]
	set_date -> set_name [label="advance [is_going_to_show] | advance [is_going_to_set_name]"]
	set_date -> set_keyword [label="advance [is_going_to_set_keyword]"]
	set_name -> set_name [label="advance [is_going_set_name]"]
	set_name -> menu [label="advance [is_going_to_search]"]
	set_keyword -> set_keyword [label="advance [is_going_set_keyword]"]
	set_keyword -> menu [label="advance [is_going_to_search]"]
}
